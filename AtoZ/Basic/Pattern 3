https://takeuforward.org/plus/dsa/beginner-problem/patterns/pattern-3

class Solution {
public:
//TC - O(N2)
//SC - O(1)
    void pattern3(int n) {
        for(int i = 1; i <= n; i++){
            for(int j = 1; j <= i; j++){
                cout << j;
            }
            cout << endl;
        }
    }
};

