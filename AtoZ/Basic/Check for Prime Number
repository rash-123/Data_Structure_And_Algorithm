https://takeuforward.org/plus/dsa/problems/check-for-prime-number

class Solution {
public:
//TC - O(N)
//SC - O(1)
    bool isPrime(int n) {
        int count = 0;
        for(int i = 1; i <= n; i++){
            if(n % i == 0){
                count++;
            }
        }
        if(count == 2){
            return true;
        }
        return false;
    }
};