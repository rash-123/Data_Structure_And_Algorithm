https://takeuforward.org/plus/dsa/beginner-problem/basic-strings/valid-anagram

class Solution{	
	public:
    //TC - O(N)
    //SC - O(1)
		bool anagramStrings(string& s,string t){
            if(s.size() != t.size()){
                return false;
            }
			vector<int> freq(256, 0);

            for(int i = 0; i < s.size(); i++){
                freq[s[i] - 'a']++;
                freq[t[i] - 'a']--;
            }

            for(int i = 0; i < 256; i++){
                if(freq[i] != 0){
                    return false;
                }
            }
            return true;
		}
};