https://takeuforward.org/plus/dsa/beginner-problem/basic-strings/largest-odd-number-in-a-string

class Solution{	
public:		
//TC - O(N)
//SC - O(N)		
    string largeOddNum(string& s){
        string ans;
        int n = s.size();
			int ind = -1;
			for(int i = n-1; i >= 0; i--){
				if((s[i] - '0') % 2 == 1){
					ind = i;
					break;
				}
			}
			int i = 0;
			while(i <= ind && s[i] == '0'){
				i++;
			}
			return s.substr(i, ind-i+1);
    }
};