https://takeuforward.org/plus/dsa/beginner-problem/basic-strings/reverse-a-string-ii

class Solution{	
	public:		
    //TC - O(N)
    //SC - O(1)
		void reverseString(vector<char>& s){
			int n = s.size();
			int left = 0, right = n-1;
			for(int i = 0; i < n; i++){
                if(left <= right){
				swap(s[left], s[right]);
				left++;
				right--;
                }
			}
		}
};