https://takeuforward.org/plus/dsa/beginner-problem/basic-arrays/reverse-an-array

class Solution{
public:
//TC - O(N)
//SC - O(1)
    void reverse(int arr[], int n){
        int left = 0, right = n-1;
        while(left < right){
            swap(arr[left], arr[right]);
            left++;
            right--;
        }
    }
};