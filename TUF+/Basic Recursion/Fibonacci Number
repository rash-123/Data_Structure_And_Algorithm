https : // takeuforward.org/plus/dsa/beginner-problem/basic-recursion/fibonacci-number

        class Solution
{
public:
    // TC - O(N)
    // SC - O(N)
    int fib(int n)
    {
        if (n == 0 || n == 1)
        {
            return n;
        }
        return fib(n - 1) + fib(n - 2);
    }
};