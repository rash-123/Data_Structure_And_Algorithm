https://takeuforward.org/plus/dsa/beginner-problem/patterns/pattern-11

class Solution {
public:
//TC - O(N2)
//SC - O(1)
    void pattern11(int n) {
        for(int i = 1; i <= n; i++){
            int start = 1;
            if(i % 2 == 0){
                    start = 0;
                }
            for(int j = 1; j <= i; j++){
                cout << start << " ";
                start = 1 - start;
            }
            cout << endl;
        }
    }
};