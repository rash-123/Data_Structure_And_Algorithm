https://takeuforward.org/plus/dsa/beginner-problem/patterns/pattern-13

class Solution {
public:
//TC - O(N2)
//SC - O(1)
    void pattern13(int n) {
        int k = 1;
        for(int i = 1; i <= n; i++){
            for(int j = 1; j <= i; j++){
                cout << k << " ";
                k++;
            }
            cout << endl;
        }
    }
};