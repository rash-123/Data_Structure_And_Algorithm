https://takeuforward.org/plus/dsa/beginner-problem/patterns/pattern-1

class Solution {
public:
//TC - O(N2)
//SC - O(1)
    void pattern1(int n) {
         for(int i = 1; i <= n; i++){
            for(int j = 1; j <= n; j++){
                cout << "*";
            }
            cout << endl;
        }
    }
};